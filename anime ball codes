function buyspin()
local ohString1 = "CrateService"
local ohString2 = "BuyCrate"
local ohTable3 = {
	[1] = "SwordA1",
	[2] = 1
}

game:GetService("ReplicatedStorage").Framework.RemoteFunction:InvokeServer(ohString1, ohString2, ohTable3)

local args = {
    [1] = "RewardService",
    [2] = "BuySpinWheel",
    [3] = {}
}

game:GetService("ReplicatedStorage").Framework.RemoteEvent:FireServer(unpack(args))
end

spawn(ballz)
task.wait(5)
while task.wait() do
spawn(buyspin)
if game.PlaceId ~= 15440283215 then
game.Players.LocalPlayer.PlayerGui.SpinWheel.Holder.Visible=true
end
end
